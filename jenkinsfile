pipeline {
    agent any
    tools { 
        maven 'Maven' 
        jdk 'jdk 1.8' 
    }
    stages {
        stage ('Building stage') {
            steps {
                echo "Building"
                  }  
                               }
         stage ('Testing stage') {
            steps {
                echo "Testing" 
                  }   
                                }
          stage ('Staging') {
            steps {
                echo "staging" 
                  }   
                            }
           stage ('Fetch from Github') {
            steps {
                git credentialsId: 'Gowthami', url: 'https://github.com/Gowthaminr/my_app' 
                  }
              }
           stage ('Initialize') {
            steps {
                sh '''
                    echo "PATH = ${PATH}"
                    echo "M2_HOME = ${M2_HOME}"
                ''' 
            }      }
           } 
            
        }
